{
  "name": "nomadeducation",
  "version": "0.5.0",
  "author": "Nomad Education",
  "description": "Javascript SDK to interact with the Nomad Education API",
  "keywords": [
    "sdk",
    "nomad",
    "education",
    "api"
  ],
  "license": "MIT",
  "main": "dist/web.js",
  "files": [
    "dist/node.js"
  ],
  "scripts": {
    "build": "webpack-cli --config build/webpack.prod.js",
    "build:test": "webpack-cli --config build/webpack.test.js",
    "lint": "eslint --quiet --cache --format codeframe .",
    "lint:fix": "eslint --fix .",
    "test": "./scripts/launch-tests.sh",
    "test:coverage": "nyc --reporter=lcovonly yarn test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:debug": "./scripts/launch-tests.sh --inspect-brk=0.0.0.0:9900",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": ">=8.x"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-cookiejar-support": "^0.4.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.1",
    "eslint": "^4.16.0",
    "eslint-config-nomadeducation": "^1.1.2",
    "faker": "^4.1.0",
    "git-revision-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "mockttp": "^0.10.0",
    "nyc": "^12.0.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  }
}
